<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="50dp"
    android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/rl_line_rate_3"
        android:layout_width="match_parent"
        android:layout_height="49dp">

        <TextView
            android:id="@+id/title"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginLeft="22.67dp"
            android:gravity="center"
            android:text="text"
            android:textColor="@color/public_select_item_dialog_text_color"
            android:textSize="16sp" />

        <ImageView
            android:id="@+id/selectIcon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="23.33dp"
            android:src="@mipmap/public_item_blue_select_icon" />

    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#F5F6F8" />


</LinearLayout>